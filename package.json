{
  "name": "state-machine-implementation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "homepage": "https://densorokin.github.io/state-machine-implementation",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "loki test",
    "test:ui:ok": "loki approve",
    "test:ui:ci": "loki --requireReference --reactUri file:./storybook-static",
    "predeploy": "npm run build:prod",
    "deploy": "gh-pages -d build",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "build:storybook": "storybook build -c ./config/storybook",
    "visual:report:json": "node scripts/generate-visual-json-report.js",
    "visual:report:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "test:ui:report": "npm run visual:report:json && npm run visual:report:html",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Denis Sorokin",
  "license": "ISC",
